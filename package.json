{
	"name": "bookself",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "npm-run-all --parallel vite typesafe-i18n ",
		"vite": "vite dev --open",
		"build": "pnpm prisma generate && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"git": "git add . && git commit -m 'update' && git push origin test",
		"typesafe-i18n": "typesafe-i18n"
	},
	"devDependencies": {
		"@stripe/stripe-js": "^1.53.0",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-vercel": "^2.4.3",
		"@sveltejs/kit": "^1.18.0",
		"@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/typography": "^0.5.9",
		"@types/bcrypt": "^5.0.0",
		"@types/nodemailer": "^6.4.8",
		"@typescript-eslint/eslint-plugin": "^5.59.6",
		"@typescript-eslint/parser": "^5.59.6",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.40.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.23",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"prettier-plugin-tailwindcss": "^0.2.8",
		"prisma": "^4.14.1",
		"stripe": "^12.5.0",
		"svelte": "^3.59.1",
		"svelte-check": "^3.3.2",
		"sveltekit-superforms": "^0.6.18",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.5.2",
		"typesafe-i18n": "^5.24.3",
		"typescript": "^5.0.4",
		"vite": "^4.3.8",
		"vitest": "^0.25.8",
		"zod": "^3.21.4"
	},
	"type": "module",
	"dependencies": {
		"@auth/core": "^0.5.1",
		"@auth/sveltekit": "^0.3.1",
		"@next-auth/prisma-adapter": "^1.0.6",
		"@prisma/client": "^4.14.1",
		"@supabase/supabase-js": "^2.22.0",
		"@trpc/client": "^10.26.0",
		"@trpc/server": "^10.26.0",
		"apexcharts": "^3.40.0",
		"bcrypt": "^5.1.0",
		"nodemailer": "^6.9.2",
		"svelte-email": "^0.0.4",
		"svelte-french-toast": "1.0.4-beta.0",
		"svelte-icons-pack": "^2.1.0",
		"trpc-sveltekit": "^3.5.7"
	}
}
